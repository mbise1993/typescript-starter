{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "Starter template for basic Typescript project",
  "author": "Your Name",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/mbise1993/typescript-starter"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:dev": "tsc",
    "build:storybook": "build-storybook",
    "format": "prettier --check 'src/**/*.?(ts|tsx)'",
    "format:fix": "npx run format -- --write",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "start:storybook": "start-storybook -p 6006",
    "test": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-info": "^5.1.11",
    "@storybook/html": "^5.1.11",
    "@types/jest": "^24.0.18",
    "@types/storybook__html": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "husky": "^3.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "source-map-explorer": "^2.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.2"
  }
}